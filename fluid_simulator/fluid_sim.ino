    if ((clk_H != prevClk_H) && (clk_H == LOW))
    {
        int dtH = digitalRead(ENCODER_DT_H);
        int deltaH = dtH == HIGH ? 1 : -1;
        updateValueH(deltaH);
        // updateColour();
    }
    prevClk_H = clk_H;
}

