void updateRotaryValue(int delta)
{
    if ((counter <= 0) && (delta < 0))
    { // going left
        counter = 19;
    }
    else if ((counter >= 19) && (delta > 0))
    { // going right
        counter = 0;
    }
    else
    { // add like normal
        counter = counter + delta;
    }
}
