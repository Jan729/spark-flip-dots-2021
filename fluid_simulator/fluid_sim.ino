#define FLUID_SIM_TIMER 100000 // total time to display fluid sim
#define FRAMES_PER_SPLASH 7 // frames for one wave period
// each splash loop starts at the same position to make the animation appear continuous

void FluidSim()
{
    // set horiztonal encoder position to middle of range
    horizontal = 16;
    int frameDelay = 125; // ms

    // measure encoder while animation is playing
    attachInterrupt(digitalPinToInterrupt(ENCODER_DT_H), ReadHorizontalEncoder, CHANGE);

    unsigned long startFluidSim = millis();

    // simulation will stop after timer runs out 
    while(millis() - startFluidSim < FLUID_SIM_TIMER) {
        // For now, alter the wave period based on the encoder value
        int splashiness = horizontal; // 0 = not splashy, 31 = very splashy
        frameDelay = map(splashiness, 0, 31, 166, 43);
        
        // TODO: When we can load images into the SD card, I will add at least 6 sets of splash
        // animation loops in order of increasing 'splashiness' and uncomment this
        // and change the wave speed / delay to "bigger wave => slower wave", "smaller wave => faster wave"
        switch(splashiness) {
            case 0 ... 4:
                animate(splash_xxl_frame_data, frameDelay, FRAMES_PER_SPLASH); //splashy_flat
                break;
            case 5 ... 8:
                animate(splash_xxl_frame_data, frameDelay, FRAMES_PER_SPLASH); //splash_xs_frame_data
                break;
            case 9 ... 12:
                animate(splash_xxl_frame_data, frameDelay, FRAMES_PER_SPLASH); //splash_s_frame_data
                break;
            case 13 ... 16:
                animate(splash_xxl_frame_data, frameDelay, FRAMES_PER_SPLASH); //splash_m_frame_data
                break;
            case 17 ... 20:
                animate(splash_xxl_frame_data, frameDelay, FRAMES_PER_SPLASH); //splash_l_frame_data
                break;
            case 21 ... 24:
                animate(splash_xxl_frame_data, frameDelay, FRAMES_PER_SPLASH); //splash_xl_frame_data
                break;
            case 25 ... 31:
                animate(splash_xxl_frame_data, frameDelay, FRAMES_PER_SPLASH);
                break;
        }

        // make the wave appear to diminish as time passes
        horizontal--;
    }

    detachInterrupt(digitalPinToInterrupt(ENCODER_DT_H));

}

void animate(uint32_t frames[][NUM_STRIPS*NUM_LEDS_PER_STRIP], int frameDelay, int aniframes){
  for(int j=0; j<aniframes; j++){
    int px = 0;
    for(int x = NUM_STRIPS - 1; x >= 0 ; x--) {
    for(int i = 0; i < NUM_LEDS_PER_STRIP; i++) {
      leds[x][i]=CRGB(pgm_read_dword(&(frames[j][px])));
      px++;
    }
  }
  FastLED.show();
    delay(frameDelay);
  }
}

void HandleClearButton()
{
    playEtchASketch = true;
    shouldClearDisplay = true;
}

void ClearDisplay()
{
    for (int row = 0; row < NUM_STRIPS; row++)
    {
        for (int col = 0; col < NUM_LEDS_PER_STRIP; col++)
        {
            leds[row][col] = CRGB::CRGB::Black;
        }
    }
    FastLED.show();
}

